<?xml version='1.0'?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
<xsl:output indent="yes"/>

<xsl:template match="/">
<LINE-COUNT>
	<xsl:for-each-group select="//SPEECH" group-by="./SPEAKER/text()">
	<xsl:sort select="current-group()/SPEAKER/text()"/>
	<SPEAKER line-count="{count(current-group()/LINE)}"><xsl:value-of select="current-grouping-key()"/></SPEAKER>	
	</xsl:for-each-group>
</LINE-COUNT>
</xsl:template>

